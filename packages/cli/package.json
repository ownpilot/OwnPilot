{
  "name": "@ownpilot/cli",
  "version": "0.1.4",
  "description": "CLI for OwnPilot",
  "type": "module",
  "bin": {
    "ownpilot": "./dist/index.js",
    "oag": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:ci": "tsc --noEmit --skipLibCheck false",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@inquirer/prompts": "^8.2.1",
    "@ownpilot/channels": "workspace:*",
    "@ownpilot/core": "workspace:*",
    "@ownpilot/gateway": "workspace:*",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1"
  },
  "optionalDependencies": {
    "@ngrok/ngrok": "^1.4.0"
  },
  "devDependencies": {
    "rimraf": "^6.1.3",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "cli",
    "ai",
    "gateway",
    "openai",
    "anthropic"
  ]
}
