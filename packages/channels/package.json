{
  "name": "@ownpilot/channels",
  "version": "0.1.0",
  "description": "Communication channels for OwnPilot (Telegram, Discord, etc.)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@ownpilot/core": "workspace:*",
    "grammy": "^1.36.0"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "vitest": "^2.1.9"
  },
  "keywords": ["telegram", "bot", "channels", "ai", "gateway"]
}
