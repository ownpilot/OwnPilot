{
  "name": "@ownpilot/ui",
  "version": "0.1.3",
  "description": "Web UI for OwnPilot",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:ci": "tsc --noEmit --skipLibCheck false",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@dagrejs/dagre": "^2.0.4",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "@xyflow/react": "^12.10.1",
    "lucide-react": "^0.575.0",
    "prism-react-renderer": "^2.4.1",
    "qrcode": "^1.5.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.1.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@types/dagre": "^0.7.53",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^5.1.4",
    "happy-dom": "^20.7.0",
    "rimraf": "^6.1.3",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^2.1.9"
  }
}
